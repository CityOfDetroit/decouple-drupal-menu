oint(min-width 768px);\n$lg: new-breakpoint(min-width 1024px);\n\n::-webkit-input-placeholder { /* Chrome/Opera/Safari */\n  color: $color-5;\n}\n::-moz-placeholder { /* Firefox 19+ */\n  color: $color-5;\n}\n:-ms-input-placeholder { /* IE 10+ */\n  color: $color-5;\n}\n:-moz-placeholder { /* Firefox 18- */\n  color: $color-5;\n}\n\n*, *:before, *:after {\n\tbox-sizing: border-box;\n\toutline: none;\n}\nbody { margin:0; padding:0; font-family: 'Montserrat', sans-serif; font-weight: lighter; overflow: hidden;}\nmain {position: relative; width: 100%;}\nh1,h2,h3,h4,h5{font-weight: bold; margin: 0;}\nimg{width: 100%;}\n// ====================== mobile ============================\n.big-menu {\n\tposition: fixed;\n\ttop: 0;\n\tright: 0;\n\tdisplay: flex;\n\talign-items: center;\n\tjustify-content: center;\n\tflex-direction: column;\n\twidth: 5em;\n\theight: 5em;\n\tbackground-color: $color-1;\n  color: $color-3;\n\t//box-shadow: 0 0 20px rgba(black,0.2);\n\ttransition: transform 250ms ease;\n\tcursor: pointer;\n\tz-index: 200;\n  font-weight: bold;\n\tletter-spacing: .2em;\n  padding: 1em 1em .5em 1em;\n\n\tspan {\n\t\tposition: relative;\n\t\tdisplay: block;\n\t\twidth: 100%;\n\t\theight: 2px;\n\t\tbackground-color: $color-3;\n\t\tfloat: left;\n\t\ttransform-origin: center center;\n\t\ttransition: transform 250ms ease;\n\t\tz-index: 200;\n\n\t\t&:nth-of-type(1) {\n\t\t\ttransform: translateY(-.5em);\n\t\t}\n\n\t\t&:nth-of-type(3) {\n\t\t\ttransform: translateY(.5em);\n      margin-bottom: .75em;\n\t\t}\n\t}\n}\n\n#big-menu {\n\tdisplay: none;\n\n\t&:checked ~ .big-menu {\n\t\tbackground-color: $color-3;\n    color: $color-1;\n\t\t// transform: rotate(360deg);\n\t\t// transition: transform 250ms ease;\n\n\t\tspan {\n\t\t\tbackground-color: $color-1;\n\t\t\ttransition: transform 250ms ease;\n\n\t\t\t&:nth-of-type(1) {\n\t\t\t\ttransform: translateY(1px) rotate(45deg);\n\t\t\t}\n\n\t\t\t&:nth-of-type(2) {\n\t\t\t\tdisplay: none;\n\t\t\t}\n\n\t\t\t&:nth-of-type(3) {\n\t\t\t\ttransform: translateY(-1px) rotate(-45deg);\n\t\t\t}\n\t\t}\n\t}\n\n\t&:checked ~ .big-nav {\n\t\tright: 0;\n\t\ttransition: right $base-duration ease;\n\t}\n\n\t&:checked ~ main {\n\t\ttransform: translateX(250px);\n\t\ttransition: transform $base-duration ease;\n\t}\n\n\t// &:checked ~ header {\n\t// \ttransform: translateX(250px);\n\t// \ttransition: transform $base-duration ease;\n\t// }\n}\n\n.big-nav {\n\tposition: fixed;\n\ttop: 0;\n\tright: -30em;\n\twidth: 30em;\n\theight: 100vh;\n  max-width: 100%;\n\tmargin: 0;\n\tpadding: 0;\n\tbackground-color: $color-2;\n\ttransition: right $base-duration ease;\n\tz-index: 5;\n\n  .search-box{\n    padding: 1em 6em 1em 1em;\n    position: relative;\n\n    input{\n      padding: 1em;\n      width: 100%;\n      border: none;\n      border-bottom: .2em solid $color-1;\n      background-color: $color-2;\n    }\n    span{\n      position: absolute;\n      width: 1em;\n      top: 1.25em;\n      right: 6.5em;\n    }\n  }\n\n  .nav-container{\n    width: 30em;\n    max-width: 100%;\n    height: calc(100vh - 5em);\n    position: fixed;\n    transition: right $base-duration ease;\n    top: 5em;\n    right: -30em;\n    background-color: $color-1;\n    overflow-y: auto;\n\n    .nav-item{\n      display: flex;\n\n      a{\n        color: $white;\n        text-decoration: none;\n        padding: .5em;\n        flex: 1;\n        display: flex;\n\n        span{\n          margin: auto 0;\n          padding: .5em;\n        }\n      }\n      a:hover{\n        background-color: $color-3;\n      }\n\n      .sub-items-btn{\n        width: 5em;\n        text-align: center;\n        padding: 1em;\n        font-weight: bold;\n        color: $white;\n        background-color: lighten($color-1, 5%);\n        cursor: pointer;\n      }\n\n      .back{\n        padding: 1em;\n        font-weight: bold;\n        color: $white;\n        background-color: lighten($color-1, 5%);\n        cursor: pointer;\n      }\n\n      .sub-items-btn:hover, .back:hover{\n        background-color: $color-3;\n      }\n    }\n  }\n\n  .active.nav-container{\n    transition: right $base-duration ease;\n    right: 0;\n  }\n}\n// ====================== mobile end ========================\n"},"lineCount":null}},"error":null,"hash":"1f873f2428c37f7fc046d63f2ec93f7a","cacheData":{"env":{}}}